{
    "name": "Bootstrap",
    "description": "Yes,it is.",
    "keywords": ["simple", "fast","modern","framework","php"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Framework\\": "framework/"
        },
        "files": [
            "libraries/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
